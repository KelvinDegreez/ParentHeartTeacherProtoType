<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kelvindegrez.parentheartteacher">

    <application
        android:name=".dagger2.MainApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".impl.splash.SplashScreenView"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_splash_screen"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".impl.home.HomeScreenView"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_home_screen_view"
            android:theme="@style/FullscreenTheme"></activity>
    </application>

</manifest>